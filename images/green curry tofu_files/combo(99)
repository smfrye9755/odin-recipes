YUI.add("trustarc-consent-banner-view",(function(e,n){require("hermes-core/flog")(n);e.FlickrView.create(this.name,e.FlickrView,[],{langBundles:this.details.langBundles,initializer:function(e){return this},loadState:function(){return e.Promise.resolve()},buildContainer:function(){return this.setContainerHTML(""),this},activate:function(){var n=e.one("#consent_blackbar");n&&(n.addClass("active"),this.appContext.adManager.infolinksActiveOnPage()&&(this.appContext.adManager.repositionInfolinksUnit("#consent_blackbar"),this.registerEventHandler(e.on("resize",e.betterThrottle((()=>{this.appContext.adManager.repositionInfolinksUnit("#consent_blackbar")}),100,this,!0)))))}})}),"@VERSION@",{requires:["promise"],optionalRequires:["hermes-core"],langBundles:["common"]});